mimic the typical html response\r\n      var hold = {\r\n        target: {value: \"\"}\r\n      };\r\n      this.handleInput(hold);\r\n    });\r\n  }\r\n\r\n  render () {\r\n    const placeholder = \"Search by \" + this.state.searchtype;\r\n    if (this.state.loading) {\r\n      return null;\r\n    } else {\r\n      console.log(\"State Check: \", this.state);\r\n      return (\r\n        <div className=\"main-body\">\r\n          <div>\r\n          <Link to = '/post'>\r\n          <button type=\"submit\">\r\n              Make A Post\r\n          </button>\r\n          </Link>\r\n          <button type=\"submit\" onClick={this.handleClick}>\r\n              Tags\r\n          </button>\r\n          {this.state.toggle ? <Tags toggle={this.handleClick} apply={this.handleIt} /> : null}\r\n          </div>\r\n          <section className=\"post-list\">\r\n            <div className=\"search-bar\">\r\n              <label for=\"search\">Search:</label>\r\n              <select name=\"options\" onChange={this.change}>\r\n                <option value=\"title\">Title</option>\r\n                <option value=\"user\">User</option>\r\n              </select>\r\n\r\n              <input type=\"text\"\r\n                className=\"search-bar-input\"\r\n                onChange={this.handleInput}\r\n                placeholder={placeholder}\r\n              />\r\n            </div>\r\n            <ul className='postlist'>\r\n              {Object.values(this.state.posts).map((post) => {\r\n                return <PostListItem title={post.title} id={post.postId} user={post.username}/>;\r\n              })}\r\n            </ul>\r\n          </section>\r\n        </div>\r\n      );\r\n    }\r\n  }\r\n}\r\n\r\nexport default Posts;\r\n","/mnt/c/programs/Databases/final/app/src/pages/postlistitem.js",[],"/mnt/c/programs/Databases/final/app/src/pages/header.js",["119","120"],"/mnt/c/programs/Databases/final/app/src/pages/postview.js",["121"],"/mnt/c/programs/Databases/final/app/src/